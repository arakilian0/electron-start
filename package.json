  "license": "MIT",
  "bugs": {
    "url": "https://github.com/arakilian0/electron-start/issues"
  },
  "homepage": "https://github.com/arakilian0/electron-start#readme",
  "devDependencies": {
    "electron": "^21.2.1"
  }
}
