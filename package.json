{
  "name": "electron-start",
  "version": "0.0.1",
  "description": "An electron.js template to use for learning Electron. Very minimally featured.",
  "main": "src/main.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
